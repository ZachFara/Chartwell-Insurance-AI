# Example .streamlit/secrets.toml file for Streamlit Cloud deployment
# Copy this to .streamlit/secrets.toml and fill in your actual values

# OpenAI Configuration
OPENAI_API_KEY = "your-openai-api-key-here"
LLAMA_CLOUD_API_KEY = "your-llama-cloud-api-key-here"

# Pinecone Configuration
PINECONE_API_KEY = "your-pinecone-api-key-here"
PINECONE_INDEX_NAME = "chartwell-insurance"
PINECONE_NAMESPACE = "llama-namespace"
PINECONE_CLOUD = "aws"
PINECONE_REGION = "us-east-1"

# LLM Configuration
LLM_MODEL = "gpt-4o"
EMBEDDING_MODEL = "text-embedding-ada-002"

# Agent Configuration
AGENT_VERBOSE = "true"
